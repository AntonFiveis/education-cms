    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "probe-image-size": "^7.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sharp": "^0.27.1",
    "uuid": "^8.3.2"
